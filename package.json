{
  "name": "series-rush",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.6.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.31",
    "@material-ui/styles": "^4.6.0",
    "axios": "^0.19.0",
    "date-fns": "^2.6.0",
    "firebase": "^7.6.0",
    "history": "^4.10.1",
    "i18next": "^17.3.1",
    "material-ui-flat-pagination": "^4.0.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.8.3",
    "react": "^16.12.0",
    "react-dom": "^16.11.0",
    "react-i18next": "^10.13.2",
    "react-redux": "^7.1.3",
    "react-redux-firebase": "^3.0.5",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-firestore": "^0.11.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "typeface-roboto": "^0.0.75"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .jsx,.js src",
    "test-coverage": "react-scripts test --coverage --watchAll=false"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "redux-mock-store": "^1.5.4"
  },
  "jest": {
    "coverageReporters": [
      "text"
    ],
    "collectCoverageFrom": [
      "src/store/app/*.js",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      },
      "src/store/app/*.js": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
